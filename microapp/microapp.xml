<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<microapp provider="arthurwynn" interface-version="1" name="crossword" version="1.0">
  <resources>
    <resource-definition name="quick" cms-path-pattern="quick/{key:number}" path="quick/{key:number}" display-name="Quick Crosswords">
      <assets>
        <asset type="text/css" path="css/crossword.css"/>
        <asset type="text/javascript" path="js/moo.js"/>
      </assets>
    </resource-definition>
  </resources>
  <keys>
    <key-definition display-name="Crossword Number" name="number"/>
  </keys>
  <components>
    <component-definition display-name="Latest Crosswords" name="latest-crosswords" path="quick/latest" widths="300,380"/>
  </components>
</microapp>
